{
  "name": "ControlVideo",
  "description": "Plays or pauses video with timers and eventually through voice",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icons8-play-16.png",
      "32": "/images/icons8-play-32.png",
      "48": "/images/icons8-play-48.png",
      "100": "/images/icons8-play-100.png"
    }
  },
  "icons": {
    "16": "/images/icons8-play-16.png",
    "32": "/images/icons8-play-32.png",
    "48": "/images/icons8-play-48.png",
    "100": "/images/icons8-play-100.png"
  }
}